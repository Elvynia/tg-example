{
  "name": "tg-example",
  "version": "1.0.0",
  "description": "Testing Trilliangular framework with a basic example",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "lite": "lite-server",
    "ntsc:w": "ntsc -w",
    "start": "ntsc && concurrently \"npm run ntsc:w\" \"npm run lite\" ",
    "prod": "npm unlink trilliangular && npm install trilliangular",
    "dev": "npm prune trilliangular && npm link trilliangular"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elvynia/tg-example.git"
  },
  "keywords": [
    "Example",
    "Angular2",
    "Three.js",
    "Trilliangular"
  ],
  "author": "Elvynia",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/platform-server": "2.0.0-rc.6",
    "core-js": "^2.4.0",
    "mousetrap": "^1.6.0",
    "mousetrap-global-bind": "^1.0.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "three": "^0.77.1",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "github-pages": "^3.0.2",
    "lite-server": "^2.2.0",
    "ntypescript": "^1.x.x",
    "typescript": "^2.0.2",
    "typings": "^1.0.4"
  }
}
